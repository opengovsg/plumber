{
  "root": false,
  "extends": [
    "../../.eslintrc.json",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended"
  ],
  "plugins": ["react", "react-hooks"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "react/display-name": "off",
    "react/react-in-jsx-scope": "off"
  }
}
