{
  "name": "backend",
  "scripts": {
    "setup": "docker-compose -f docker-compose.dev.yml up -d",
    "teardown": "docker-compose -f docker-compose.dev.yml down",
    "dev": "npm run build:watch -- --exec \"ts-node src/server.ts\"",
    "worker": "npm run build:watch -- --exec \"ts-node src/worker.ts\"",
    "prebuild": "rm -rf ./dist",
    "build": "tsc && npm run copy-statics && tsc-alias",
    "build:watch": "nodemon --watch \"src/**\" --ext \"ts,json\"",
    "start": "node dist/server.js",
    "start:worker": "node dist/worker.js",
    "test:unit": "vitest --config ./vitest.config.ts",
    "test:integration": "vitest --config ./vitest.config.integration.ts",
    "lint": "eslint . --ignore-path ../../.eslintignore",
    "db": "DOTENV_CONFIG_PATH=.env knex",
    "db:migration:create": "npm run db -- migrate:make",
    "db:list": "npm run db -- migrate:list",
    "db:rollback": "npm run db -- migrate:rollback",
    "db:migrate": "npm run db -- migrate:latest",
    "copy-statics": "copyfiles -u 1 src/**/*.{graphql,json,svg} dist"
  },
  "dependencies": {
    "@apollo/server": "4.9.4",
    "@aws-sdk/client-s3": "3.369.0",
    "@bull-board/express": "5.6.0",
    "@graphql-tools/schema": "10.0.0",
    "@launchdarkly/node-server-sdk": "9.0.4",
    "@opengovsg/formsg-sdk": "0.11.0",
    "@opengovsg/sgid-client": "2.1.0",
    "@plumber/types": "file:../types",
    "ajv-formats": "^2.1.1",
    "async-mutex": "0.4.0",
    "axios": "1.6.2",
    "bullmq": "^3.0.0",
    "cookie-parser": "1.4.6",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "csv-parse": "5.4.0",
    "dd-trace": "4.17.0",
    "dedent": "1.5.1",
    "dotenv": "^10.0.0",
    "email-validator": "2.0.4",
    "express": "4.18.2",
    "express-basic-auth": "^1.2.1",
    "form-data": "4.0.0",
    "graphql": "16.8.1",
    "graphql-middleware": "6.1.35",
    "graphql-rate-limit": "3.3.0",
    "graphql-shield": "7.6.5",
    "graphql-type-json": "^0.3.2",
    "helmet": "6.1.5",
    "http-errors": "~1.6.3",
    "ipaddr.js": "2.1.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "2.4.2",
    "lodash.get": "^4.4.2",
    "lottie-web": "5.12.2",
    "luxon": "2.5.2",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "nanoid": "3.3.6",
    "objection": "^3.0.0",
    "pg": "^8.7.1",
    "rate-limiter-flexible": "2.4.1",
    "redlock": "5.0.0-beta.2",
    "remark-breaks": "3.0.3",
    "remark-gfm": "3.0.1",
    "typescript": "^4.6.3",
    "winston": "^3.7.1",
    "zod": "3.22.4",
    "zod-validation-error": "1.5.0"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^10.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.8",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.0.2",
    "@types/express": "^4.17.15",
    "@types/http-errors": "^1.8.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash.get": "^4.4.6",
    "@types/luxon": "^2.4.0",
    "@types/morgan": "^1.9.3",
    "@types/multer": "1.4.7",
    "@types/pg": "^8.6.1",
    "nodemon": "^2.0.22",
    "testcontainers": "^10.2.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0"
  },
  "version": "1.8.7"
}
